{
  "ignore": [
    "webpack"
  ],
  "lint": {
    "h-parent": "parent",
    "h-tag": "tag"
  },
  "media-providers": {
    "github": {
      "default-for": ["image"],
      "title-from-src": true,
      "path": "../media",
      "remote": "cirosantilli/media"
    },
    "wikimedia": {
      "title-from-src": true
    }
  },
  "h": {
    "numbered": false,
    "splitDefault": true
  },
  "outputOutOfTree": true,
  "template": "main.liquid.html",
  "unsafe-xss": true
}
