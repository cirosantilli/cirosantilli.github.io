{
  "ignoreConvert": [
    ".*\\.scss"
  ],
  "dontIgnoreConvert": [
    "main.scss"
  ],
  "lint": {
    "h-parent": "parent",
    "h-tag": "tag"
  },
  "media-providers": {
    "github": {
      "default-for": ["image"],
      "title-from-src": false,
      "path": "../media",
      "remote": "cirosantilli/media"
    },
    "wikimedia": {
      "title-from-src": false
    }
  },
  "h": {
    "numbered": false
  },
  "redirects": [
    ["cirodown", "ourbigbook"],
    ["ourbigbook", "https://docs.ourbigbook.com"]
  ],
  "publishCommitDate": "2000-01-01T00:00:00",
  "publishRemoteUrl": "git@github.com:cirosantilli/cirosantilli.github.io.git",
  "publishOptions": {
    "toSplitHeaders": true,
    "htmlXExtension": false,
    "xPrefix": "https://ourbigbook.com/cirosantilli/"
  },
  "unsafe-xss": true,
  "web": {
    "linkFromHeaderMeta": true,
    "username": "cirosantilli"
  }
}
