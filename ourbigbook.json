{
  "ignore": [
    ".venv",
    "webpack"
  ],
  "lint": {
    "h-parent": "parent",
    "h-tag": "tag"
  },
  "media-providers": {
    "github": {
      "default-for": ["image"],
      "title-from-src": false,
      "path": "../media",
      "remote": "cirosantilli/media"
    },
    "wikimedia": {
      "title-from-src": false
    }
  },
  "h": {
    "numbered": false
  },
  "outputOutOfTree": true,
  "redirects": [
    ["cirodown", "ourbigbook"],
    ["ourbigbook", "https://docs.ourbigbook.com"]
  ],
  "_reroutePrefix": "https://ourbigbook.com/cirosantilli/",
  "template": "main.liquid.html",
  "unsafe-xss": true
}
